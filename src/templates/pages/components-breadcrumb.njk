{% extends "layouts/default.njk" %}

{% set DOC_TITLE = "Component: Breadcrumb" %}
{% set BASE_HREF = "./" %}
{% set BODY_CLASS = "fixed-navbar" %}

{% block main %}
<div class="main-content">
   <div class="row">
      <div class="col mb-3">
         <div class="d-block">
            <h2 class="main-content__title mb-0">Component: Breadcrumb</h2>
            <nav aria-label="breadcrumb">
               <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="./dashboard.html">Home</a></li>
                  <li class="breadcrumb-item">Components</li>
                  <li class="breadcrumb-item active">Breadcrumb</li>
               </ol>
            </nav>
         </div>
      </div><!-- /.col -->
   </div><!-- /.row -->
   <div class="row">
      <div class="col">
         <div class="box box--full-height">
            <div class="box__wrapper">
               <div class="box__header">
                  <h3 class="box__title">Default Breadcrumb</h3>
                  <div class="box__action">
                     <button class="button button--link js-button-reload">
                        <i data-feather="rotate-cw"></i>
                     </button>
                     <button class="button button--link js-button-expand">
                        <i data-feather="maximize"></i>
                     </button>
                  </div>
               </div>
               <div class="box__body">
                  <div class="pxb-20px">
                     <p class="box__text">Breadcrumb pattern menggunakan default syntax Bootstrap 4.1 dengan adjustment styling. Take note class <code class="language-css">"mb-0"</code> hanya dipakai untuk kebutuhan dokumentasi saja.</p>
                     <div class="highlight">
                        <h2>Breadcrumb</h2>
                        <nav aria-label="breadcrumb">
                           <ol class="breadcrumb mb-0">
                              <li class="breadcrumb-item"><a href="./dashboard.html">Home</a></li>
                              <li class="breadcrumb-item">Components</li>
                              <li class="breadcrumb-item active">Breadcrumb</li>
                           </ol>
                        </nav>
                     </div>
                     <div class="language-html">
<pre><code>&lt;nav aria-label="breadcrumb">
   &lt;ol class="breadcrumb">
      &lt;li class="breadcrumb-item">&lt;a href="./dashboard.html">Home&lt;/a>&lt;/li>
      &lt;li class="breadcrumb-item">Components&lt;/li>
      &lt;li class="breadcrumb-item active">Breadcrumb&lt;/li>
   &lt;/ol>
&lt;/nav>
</code></pre>
</div>
                  </div>
               </div>
            </div>
         </div>
      </div> <!-- /.col -->
   </div><!-- /.row -->  
   <div class="row">
      <div class="col">
         <div class="box box--full-height">
            <div class="box__wrapper">
               <div class="box__header">
                  <h3 class="box__title">Breadcrumb Alternative</h3>
                  <div class="box__action">
                     <button class="button button--link js-button-reload">
                        <i data-feather="rotate-cw"></i>
                     </button>
                     <button class="button button--link js-button-expand">
                        <i data-feather="maximize"></i>
                     </button>
                  </div>
               </div>
               <div class="box__body">
                  <div class="pxb-20px">
                     <div class="highlight">
                        <h2>Breadcrumb</h2>
                        <nav aria-label="breadcrumb">
                           <ol class="breadcrumb breadcrumb--alt mb-0">
                              <li class="breadcrumb-item"><a href="./dashboard.html">Home</a></li>
                              <li class="breadcrumb-item">Components</li>
                              <li class="breadcrumb-item active">Breadcrumb</li>
                           </ol>
                        </nav>
                     </div>
                     <div class="language-html">
<pre><code>&lt;nav aria-label="breadcrumb">
   &lt;ol class="breadcrumb breadcrumb--alt">
      &lt;li class="breadcrumb-item">&lt;a href="./dashboard.html">Home&lt;/a>&lt;/li>
      &lt;li class="breadcrumb-item">Components&lt;/li>
      &lt;li class="breadcrumb-item active">Breadcrumb&lt;/li>
   &lt;/ol>
&lt;/nav>
</code></pre>
</div>
                  </div>
               </div>
            </div>
         </div>
      </div> <!-- /.col -->
   </div><!-- /.row -->   
   {% include "components/_footer.njk" %}
</div>
{% endblock %}

{% block additional_scripts %}{% endblock %}